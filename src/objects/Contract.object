        <fullName>GeneratedOrder</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>生成订单</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)}
if ({!ISPICKVAL(Contract.ContractReviewStatus__c, &apos;草稿&apos;)} || {!ISPICKVAL(Contract.ContractReviewStatus__c, &apos;待审批&apos;)} || {!ISPICKVAL(Contract.ContractReviewStatus__c, &apos;已拒绝&apos;)})
{
	alert(&apos;合同尚未完成评审！&apos;);
}
else if(&apos;{!Contract.customerfirst__c}&apos; == &apos;否&apos;)
{
	
	alert(&apos;不是首营客户，请确认客户是否同步到NC，是否有首营信息！&apos;);
}
else
{
	var currentOperURL = window.location.href; 
	var result = sforce.apex.execute(&quot;GeneratedOrderWebService&quot;, &quot;generatedOrder&quot;,{contractId:&quot;{!Contract.Id}&quot;}); 

	if(result[0].IsSuccess == &apos;true&apos;){ 
	alert(result[0].Message); 

	} 
	else { 
	alert(result[0].Message); 
	} 
	window.location.reload(true);
	
}</url>
