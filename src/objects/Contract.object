        <masterLabel>同步客户</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)}
if (&apos;{!Contract.customerfirst__c}&apos; == &apos;否&apos;)
{
	var currentOperURL = window.location.href; 
	var result = sforce.apex.execute(&quot;SyncAccountInfWebService&quot;, &quot;syncAccountInfo&quot;,{accountId:&quot;{!Contract.AccountId}&quot;}); 

	if(result[0].IsSuccess == &apos;true&apos;){ 
		var isRedirect = confirm(&apos;客户信息同步结束，是否跳转到客户页面查看同步结果？&apos;);
		console.log(&apos;isRedirect is &apos; + isRedirect);
		console.log(&apos;isRedirect is &apos; + (isRedirect == true));
		if(isRedirect == true)
		{
			
			window.location.href = &apos;/{!Contract.AccountId}&apos;;;
		}
		
	} 
	else { 
		alert(result[0].Message); 
		window.location.reload(true);
	} 
	
}
else
{
	alert(&apos;客户已经同步，请勿重复同步！&apos;);
}</url>
