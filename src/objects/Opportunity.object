        <fullName>NewOpportunityOnNormal</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Opportunity</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)} 
var forcastRecordTypeId = sforce.connection.query(&quot;Select id From recordtype where Name = &apos;预测用常规销售机会&apos; limit 1&quot;) ; 
var userRecordTypeid = sforce.connection.query(&quot;Select id From recordtype where Name = &apos;常规销售机会&apos; limit 1&quot;) ; 
  console.log(&apos;forcastRecordTypeId is &apos; + forcastRecordTypeId);
  console.log(&apos;userRecordTypeid is &apos; + userRecordTypeid);
 
var redirectURL = &quot;/006/e?CF00N6F00000Lcxcl={!Opportunity.Name}&amp;CF00N6F00000Lcxcl_lkid={!Opportunity.Id}&amp;&quot; //父机会的Id 和名字
				 + &quot;opp4={!Opportunity.Account}&amp;opp4_lkid={!Opportunity.AccountId}&amp;&quot; //客户名称和Id
				 + &quot;CF00N6F00000Ec7YI={!Opportunity.FinalCustomer__c}&amp;CF00N6F00000Ec7YI_lkid={!Opportunity.FinalCustomerId__c}&amp;&quot; //最终用户名称和Id
				 + &quot;opp3={!Opportunity.Name}&amp;&quot; //销售机会名称，和父销售机会一致
				 + &quot;opp11={!Opportunity.StageName}&amp;&quot; //阶段
				 + &quot;opp12={!Opportunity.Probability}&amp;&quot;//可能性
				 + &quot;00N6F00000Ebs6D={!Opportunity.BusinessSource__c}&amp;&quot; //商机来源
				 + &quot;opp9={!Opportunity.CloseDate}&amp;&quot; //预计成交日期
                                 + &quot;00N6F00000Ebs5y={!Opportunity.ProcurementMethod__c}&amp;&quot;
	             + &quot;retURL=%2F{!Opportunity.Id}&amp;ent=Opportunity&amp;RecordType=&quot;;
 

if (&apos;{!Opportunity.RecordType}&apos; == &apos;常规销售机会&apos;)
{
	redirectURL = redirectURL + forcastRecordTypeId.records.Id ;
	console.log(&apos;current url is &apos; + redirectURL);
	window.parent.navigateToUrl(redirectURL); 
}
else if (&apos;{!Opportunity.RecordType}&apos; == &apos;常规销售机会（大区经理）&apos;)
{
	redirectURL = redirectURL + forcastRecordTypeId.records.Id ;
	console.log(&apos;current url is &apos; + redirectURL);
	window.parent.navigateToUrl(redirectURL); 
	
}
else 
{
 alert(&apos;只有常规销售机会和常规销售机会（大区经理）才能创建子销售机会！&apos;);
}</url>
    </webLinks>
