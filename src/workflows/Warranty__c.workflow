    <rules>
        <fullName>warranty  contract</fullName>
        <actions>
            <name>congratulation</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Warranty__c.Warrantystartdate__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>保修合约分期付款提醒东区</fullName>
