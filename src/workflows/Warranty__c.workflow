        </criteriaItems>
        <criteriaItems>
            <field>Warranty__c.areanew__c</field>
            <operation>equals</operation>
            <value>East</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>NextPayDateRemindEast</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Warranty__c.NextPayDate__c</offsetFromField>
            <timeLength>-60</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
